    <application
        android:name=".mode.CurrentMode"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectModeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagerHomeActivity" />
        <activity android:name=".AddPlayerActivity" />
        <activity android:name=".PlayerListActivity" />
        <activity android:name=".PlayerProfileActivity" />
        <activity android:name=".StartTournamentActivity" />
        <activity android:name=".PlayerHomeActivity" />
        <activity android:name=".ViewPastProfitActivity" />
    </application>

</manifest>